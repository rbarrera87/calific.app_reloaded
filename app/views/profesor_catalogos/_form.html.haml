= simple_form_for @profesor_catalogo, :html => { :class => 'form-horizontal' } do |f|
  = f.input :tipo_horario
  = f.input :estado
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), profesor_catalogos_path, :class => 'btn'
