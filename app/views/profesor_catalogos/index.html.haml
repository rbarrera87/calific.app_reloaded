- model_class = ProfesorCatalogo
.page-header
  %h1=t '.title', :default => model_class.model_name.human.pluralize.titleize
%table.table.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:id)
      %th= model_class.human_attribute_name(:tipo_horario)
      %th= model_class.human_attribute_name(:estado)
      %th= model_class.human_attribute_name(:created_at)
      %th=t '.actions', :default => t("helpers.actions")
  %tbody
    - @profesor_catalogos.each do |profesor_catalogo|
      %tr
        %td= link_to profesor_catalogo.id, profesor_catalogo_path(profesor_catalogo)
        %td= profesor_catalogo.tipo_horario
        %td= profesor_catalogo.estado
        %td= profesor_catalogo.created_at
        %td
          = link_to t('.edit', :default => t("helpers.links.edit")), edit_profesor_catalogo_path(profesor_catalogo), :class => 'btn btn-mini'
          = link_to t('.destroy', :default => t("helpers.links.destroy")), profesor_catalogo_path(profesor_catalogo), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-mini btn-danger'

= link_to t('.new', :default => t("helpers.links.new")), new_profesor_catalogo_path, :class => 'btn btn-primary'
