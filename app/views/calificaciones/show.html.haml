- model_class = Calificacion
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @calificacion.user_id
%p
  %strong= model_class.human_attribute_name(:parcial_id) + ':'
  %br
  = @calificacion.parcial_id
%p
  %strong= model_class.human_attribute_name(:indicador_id) + ':'
  %br
  = @calificacion.indicador_id
%p
  %strong= model_class.human_attribute_name(:asignatura_id) + ':'
  %br
  = @calificacion.asignatura_id
%p
  %strong= model_class.human_attribute_name(:valor) + ':'
  %br
  = @calificacion.valor

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), calificaciones_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_calificacion_path(@calificacion), :class => 'btn'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), calificacion_path(@calificacion), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
